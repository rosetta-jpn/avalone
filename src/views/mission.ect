<div id="mission" class="scene">
  <h2 class="page-header">Mission</h2>
  <button class="btn btn-default debug debug-success">Debug (Success)</button>
  <button class="btn btn-default debug debug-failure">Debug (Failure)</button>
  <div align="center" class="rv-quest" rv-show="quest.team#isMember">
    <a href="#" rv-on-click="handlers#submitSuccess"><img src="images/Success.jpg" alt="Success" style="margin-right:200px;"></a>
    <a href="#" rv-on-click="handlers#submitFail"><img src="images/Failure.jpg" alt="Fail"></a>
  </div>
</div>
