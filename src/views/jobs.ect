<div id="jobs" class="scene">
  <h2 class="page-header">Your job</h2>
  <div class="profile">
    <h3>{{ player.name }}</h3>
    <div align="center">
      <p>
        <img v-attr="src:player.class | imagePath">
      </p>
    </div>
  </div>
  <h2 class="page-header">Others jobs</h2>
  <% include "players_jobs" %>
  <br>
  <div class="col-lg-6" align="right">
    <p>
      <a class="btn btn-lg btn-success" role="button" id="go_team">Go Team</a>
    </p>
  </div>
</div>

