 <div id="vote" class="scene">
  <% include "team_select_info" %>
  <% include "team_member" %>
  <br>
  <div class="rv-team">
    <form role="form" rv-hide="team.isVoted">
      <label class="radio-inline">
        <input type="radio" id="approve" name="votes" rv-checked="team.isVoteApprove">
        <img src="images/Approve.jpg" alt="Approve">
      </label>
      <label class="radio-inline">
        <input type="radio" id="reject" name="votes" rv-unchecked="team.isVoteApprove">
        <img src="images/Reject.jpg" alt="Reject">
      </label>
      <button type="submit" rv-on-click="handlers#submitVote" class="btn btn-primary">Vote!!</button>
    </form>
    <div rv-show="team.isVoted">
      <div rv-show="team.isVoteApprove">
        <img src="images/Approve.jpg" alt="Approve">
      </div>
      <div rv-hide="team.isVoteApprove">
        <img src="images/Reject.jpg" alt="Reject">
      </div>
    </div>
  </div>
  <br><br>
  <button class="btn btn-default debugApprove debug">Debug (Approve)</button>
  <button class="btn btn-default debugReject debug">Debug (Reject)</button>
</div>
